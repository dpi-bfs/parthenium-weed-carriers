{
  "compilerOptions": {
    "target": "ES2015",
    "module": "Node16",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "outDir": "out",
    "baseUrl": ".",
    "sourceMap": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/embedding/*"
  ]  
}